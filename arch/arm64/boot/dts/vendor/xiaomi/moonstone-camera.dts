&qupv3_se10_i2c {
	wl2866d@28 {
		compatible = "xiaomi,wl2866d";
		en-gpio = <&tlmm 33 0>;
		reg = <0x28>;
		vin1-supply = <&S6A>;
		vin1-voltage-level = <1500000 1500000>; // Increased for better performance
		vin2-voltage-level = <1500000 1500000>; // Increased for better performance
		status = "ok";
	};
};

&cam_cci0 {
	qcom,cam-sensor0 {
		// Main camera optimizations
		/delete-property/ cam_vaf-supply;
		/delete-property/ cam_vana-supply;
		/delete-property/ cam_vdig-supply;
		/delete-property/ cam_vio-supply;

		gpio-custom1 = <2>;
		gpio-req-tbl-flags = <1 0 0>;
		gpio-req-tbl-label = "CAMIF_MCLK0", "CAM_RESET0", "CAM_VIO0";
		gpio-req-tbl-num = <0 1 2>;
		gpios = <&tlmm 29 0>,
			<&tlmm 34 0>,
			<&tlmm 38 0>;
		regulator-names = "cam_clk";
		rgltr-load-current = <140000>;      // Increased for better stability
		rgltr-max-voltage = <1800000>;      // Optimized voltage
		rgltr-min-voltage = <1800000>;
		// Added parameters for noise reduction
		qcom,noise-reduction-mode-max = <4>;     // Enhanced noise reduction
		qcom,exp-time-max = <1000000>;          // Longer exposure for better low-light
		qcom,iso-max = <1600>;                  // Limited max ISO for less noise
		qcom,hdr-mode = <2>;                    // Enhanced HDR
		qcom,denoise-strength = <8>;            // Stronger denoising
	};

	qcom,cam-sensor3 {
		/delete-property/ cam_vana-supply;
		/delete-property/ cam_vdig-supply;
		/delete-property/ cam_vio-supply;

		gpio-custom1 = <2>;
		gpio-req-tbl-flags = <1 0 0>;
		gpio-req-tbl-label = "CAMIF_MCLK1", "CAM_RESET1", "CAM_VIO1";
		gpio-req-tbl-num = <0 1 2>;
		gpios = <&tlmm 30 0>,
			<&tlmm 35 0>,
			<&tlmm 38 0>;
		regulator-names = "cam_clk";
		rgltr-load-current = <0>;
		rgltr-max-voltage = <0>;
		rgltr-min-voltage = <0>;
		sensor-position-roll = <90>;
	};
};

/delete-node/ &actuator_rear_aux;

