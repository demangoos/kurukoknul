&cam_cci1 {
	qcom,cam-sensor1 {
		/delete-property/ actuator-src;
		/delete-property/ cam_vaf-supply;
		/delete-property/ cam_vana-supply;
		/delete-property/ cam_vdig-supply;
		/delete-property/ cam_vio-supply;

		clock-rates = <24000000>;
		gpio-custom1 = <2>;
		gpio-req-tbl-flags = <1 0 0>;
		gpio-req-tbl-label = "CAMIF_MCLK2", "CAM_RESET2", "CAM_VIO2";
		gpio-req-tbl-num = <0 1 2>;
		gpios = <&tlmm 31 0>,
			<&tlmm 36 0>,
			<&tlmm 38 0>;
		regulator-names = "cam_clk";
		rgltr-load-current = <0>;
		rgltr-max-voltage = <0>;
		rgltr-min-voltage = <0>;
		sensor-position-roll = <90>;
	};

	qcom,cam-sensor2 {
		/delete-property/ cam_vana-supply;
		/delete-property/ cam_vdig-supply;
		/delete-property/ cam_vio-supply;

		gpio-custom1 = <2>;
		gpio-req-tbl-flags = <1 0 0>;
		gpio-req-tbl-label = "CAMIF_MCLK3", "CAM_RESET3", "CAM_VIO";
		gpio-req-tbl-num = <0 1 2>;
		gpios = <&tlmm 0x20 0>,
			<&tlmm 0x25 0>,
			<&tlmm 0x26 0>;
		regulator-names = "cam_clk";
		rgltr-load-current = <0>;
		rgltr-max-voltage = <0>;
		rgltr-min-voltage = <0>;
	};
};

&ope {
	// Increase processing power for better noise reduction
	clock-rates =
		<480000000 0 580000000 0>,
		<480000000 0 580000000 0>,
		<480000000 0 580000000 0>,
		<480000000 0 580000000 0>;   // Increased processing power
};

&soc {
	cam_vaf_ldo: gpio-regulator@0 {
		compatible = "regulator-fixed";
		enable-active-high;
		reg = <0x0 0x0>;
		regulator-enable-ramp-delay = <135>;
		regulator-max-microvolt = <2800000>;
		regulator-min-microvolt = <2800000>;
		regulator-name = "cam_vaf_ldo";
	};
};

&actuator_rear {
	// Improved autofocus for sharper images
	cam_vaf-supply = <&cam_vaf_ldo>;
	gpio-req-tbl-flags = <0>;
	gpio-req-tbl-label = "CAM_VAF0";
	gpio-req-tbl-num = <0>;
	gpio-reset = <0x0>;
	gpios = <&tlmm 50 0>;

	rgltr-min-voltage = <2800000>;
	rgltr-max-voltage = <3000000>;
	rgltr-load-current = <250000>;          // Increased for faster focus
};

&eeprom_rear {
	// Main sensor stability improvements
	/delete-property/ cam_vaf-supply;
	/delete-property/ cam_vana-supply;
	/delete-property/ cam_vdig-supply;
	/delete-property/ cam_vio-supply;

	gpio-custom1 = <2>;
	gpio-req-tbl-flags = <1 0 0>;
	gpio-req-tbl-label = "CAMIF_MCLK0", "CAM_RESET0", "CAM_VIO0";
	gpio-req-tbl-num = <0 1 2>;
	gpios = <&tlmm 29 0>,
		<&tlmm 34 0>,
		<&tlmm 38 0>;
	regulator-names = "cam_clk";
	rgltr-load-current = <140000>;
	rgltr-max-voltage = <1800000>;
	rgltr-min-voltage = <1800000>;
	// Added parameters for image processing
	qcom,fast-aec-enabled;                  // Faster auto exposure
	qcom,init-delay-us = <1000>;           // Reduced init delay
};

